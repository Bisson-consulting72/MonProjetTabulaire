            }
          },
          {
            "name": "Taux marge nette",
            "expression": " ([Marge Brute] + SUM(Ventes[Taxes]) + SUM(Ventes[Transports]))/SUM(Ventes[Montant ventes HT])",
            "formatString": "0.00 %;-0.00 %;0.00 %",
            "displayFolder": "Marge"
          },
          {
            "name": "KPI Profit Tendance",
            "expression": [
              "",
              "IF ([Taux marge nette]",
              "        > CALCULATE ([Taux marge nette],",
              "            PARALLELPERIOD ( Temps[Date complète], -1, YEAR )",
              "        ),",
              "    1,",
              "    IF ([Taux marge nette]",
              "            < CALCULATE ([Taux marge nette],",
              "                PARALLELPERIOD ( Temps[Date complète], -1, YEAR )",
              "            ),",
              "        -1,",
              "        0",
              "    )",
              ")",
              ""
            ],
            "displayFolder": "Marge",
            "kpi": {
              "targetExpression": "1",
              "statusGraphic": "Road Signs",
              "statusExpression": [
                "var x='Ventes'[KPI Profit Tendance] return",
                "            if(ISBLANK(x),BLANK(),",
                "                    If(x<-1,-1,",
                "\t                If(x<1,0,1)",
                "                )",
                "            )",
                "            "
              ],
