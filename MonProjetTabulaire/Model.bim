        "measures": [
          {
            "name": "Marge Brute",
            "expression": "SUM(Ventes[Montant ventes HT])-SUM(Ventes[Coûts produits])",
            "formatString": "#,0.00 \"€\";-#,0.00 \"€\";#,0.00 \"€\"",
            "displayFolder": "Marge",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"Currency\" Accuracy=\"2\"><Currency LCID=\"1036\" DisplayName=\"€ Français (France)\" Symbol=\"€\" PositivePattern=\"3\" NegativePattern=\"8\" /></Format>"
              }
            ]
          },
          {
            "name": "Nb ligne cde",
            "expression": "COUNTA([N° ligne cde])",
            "formatString": "#,0"
          },
          {
            "name": "Taux de marge",
            "expression": "[Marge Brute] / SUM(Ventes[Montant ventes HT])",
            "formatString": "0.00 %;-0.00 %;0.00 %",
            "displayFolder": "Marge"
          },
          {
            "name": "Taux de marque",
            "expression": "DIVIDE([Marge Brute], SUM(Ventes[Coûts produits]))",
            "formatString": "0.00 %;-0.00 %;0.00 %",
            "displayFolder": "Marge"
          },
          {
            "name": "Coeff marge",
