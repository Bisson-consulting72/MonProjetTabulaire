          },
          {
            "name": "KPI Profit Objectif",
            "expression": " SWITCH(SELECTEDVALUE('Régions'[Régions]),\"Canada\",0.34, \"Germany\", 0.36, \"France\", 0.32, 0.30)",
            "displayFolder": "Marge"
          },
          {
            "name": "KPI Profit",
            "expression": " [Taux marge nette]/[KPI Profit Objectif]",
            "displayFolder": "Marge",
            "kpi": {
              "targetExpression": "1,5",
              "statusGraphic": "Traffic Light - Single",
              "statusExpression": [
                "var x='Ventes'[KPI Profit] return",
                "            if(ISBLANK(x),BLANK(),",
                "                    If(x<1.3,-1,",
                "\t                If(x<1.5,0,1)",
                "                )",
                "            )",
                "            "
              ],
              "annotations": [
