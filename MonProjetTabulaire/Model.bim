                  "value": "-1"
                },
                {
                  "name": "KpiThreshold_1",
                  "value": "1"
                }
              ]
            }
          },
          {
            "name": "Qté expédiée",
            "expression": " CALCULATE(SUM(Ventes[Qté commandée]),USERELATIONSHIP(Temps[Date],Ventes[Date expédition]))",
